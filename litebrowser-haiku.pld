NAME=litebrowser-haiku
TARGETNAME=litebrowser
PLATFORM=HaikuGCC4
SCM=git
GROUP=Source files
EXPANDGROUP=yes
SOURCEFILE=src/App.cpp
SOURCEFILE=src/LiteHtmlView.cpp
SOURCEFILE=src/MainWindow.cpp
LOCALINCLUDE=.
SYSTEMINCLUDE=B_FIND_PATH_DEVELOP_HEADERS_DIRECTORY/be
SYSTEMINCLUDE=B_FIND_PATH_DEVELOP_HEADERS_DIRECTORY/cpp
